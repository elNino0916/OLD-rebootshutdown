@echo off
mkdir %appdata%\elninosoftware\
mkdir %appdata%\elninosoftware\rebootshutdown
cls
echo elNino0916
echo.
echo RebootShutdown v.DEV
echo.
cd %appdata%\elninosoftware\rebootshutdown
tasklist>programlistner.log
set input=
set /p input= Reboot, Shutdown or Logoff:  
if %input%==Reboot goto shutdown -r -f -t 1
if %input%==Shutdown shutdown -s -f -t 1
if %input%==Logoff logoff
exit 
